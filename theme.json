            "customGradient": true,
            "link": true,
            "palette": [
                {
                    "slug": "black",
                    "color": "#000",
                    "name": "Black"
                },
                {
                    "slug": "white",
                    "color": "#fff",
                    "name": "White"
                },
                {
                    "slug": "dark-blue",
                    "color": "#1d1f35",
                    "name": "Dark blue"
                },
                {
                    "slug": "light-blue",
                    "color": "#6cace4",
                    "name": "Light blue"
                },
                {
                    "slug": "beige",
                    "color": "#c7c0af",
                    "name": "Beige"
                },
                {
                    "slug": "silver",
