        "blocks": {

            "core/post-title": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "fontSize": "var(--wp--preset--font-size--large)"
                        }
                    }
                }
            },
            "core/paragraph": {
                "spacing": {
                    "blockGap": "20px",
                    "margin": {
                        "top": "0",
                        "right": "0",
                        "left": "0"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
