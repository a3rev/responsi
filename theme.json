                }
            },
            "core/button": {
                "color": {
                    "background": "var(--wp--preset--color--footer-yellow)",
                    "text": "var(--wp--preset--color--white)"
                },
                "spacing": {
                    "blockGap": "20px",
                    "padding": {
                        "top": "var(--wp--custom--button-padding-top)",
                        "right": "var(--wp--custom--button-padding-right)",
                        "bottom": "var(--wp--custom--button-padding-bottom)",
                        "left": "var(--wp--custom--button-padding-left)"
                    }
                },
                "border": {
                    "width": "1px",
                    "radius": "4px",
                    "style":"solid",
                    "color":"var(--wp--preset--color--footer-yellow)"
                },
                "typography": {
                    "lineHeight": "inherit"
                }
            },
            "core/social-links": {
                "spacing": {
                    "blockGap": "5px"
                }
            },
            "core/query-pagination": {
