{
    "version": 2,
    "settings": {

        "blocks": {
            "core/paragraph": {
                "supports": {
                    "spacing": {
                        "padding": true,
                        "margin": true
                    },
                    "typography": {
                        "__experimentalFontFamily": true
                    }
                }

            },
            "core/heading": {
                "supports": {
                    "spacing": {
                        "padding": true,
                        "margin": true
                    },
                    "typography": {
                        "fontSize":true,
