                    "name": "medium"
                },
                {
                    "slug": "large",
                    "size": "24px",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "28px",
                    "name": "X Large"
                },
                {
                    "slug": "xx-large",
                    "size": "32px",
                    "name": "XX Large"
                },
                {
                    "slug": "extra-large",
                    "size": "40px",
                    "name": "Extra large"
                },
                {
                    "slug": "huge",
                    "size": "96px",
                    "name": "Huge"
                }
            ]
        },
      
        "layout": {
           "contentSize": "var(--responsive--contentwide-width)",
            "wideSize": "var(--responsive--alignwide-width)",
            "units": [ "px", "em", "rem", "%", "vw", "vh" ]
        },

        "border": {
            "customColor": true,
            "customRadius": true,
            "customStyle": true,
            "customWidth": true
        },
        "custom":{
            "gutterHorizontal":"20px",
            "gutterVertical":"20px",

            "responsiveSpaceHorizontal":"20px",
            "responsiveSpaceVertical":"20px",

            "marginTop":"20px",
            "marginRight":"20px",
            "marginBottom":"20px",
            "marginLeft":"20px",

            "paddingTop":"20px",
            "paddingRight":"20px",
            "paddingBottom":"20px",
